{
  "name": "@vseplet/fetchify",
  "version": "0.3.20",
  "workflows": {
    "UpdateSemverDeployJsr": {
      "versionsFilePath": "./source/versions.ts"
    }
  },
  "exports": {
    ".": "./source/mod.ts",
    "./types": "./source/types.ts",
    "./Fetchify": "./source/Fetch.ts",
    "./Limiter": "./source/Limiter.ts"
  },
  "lock": false,
  "tasks": {
    "ex2": "deno run -A ./examples/ex2.ts",
    "ex5": "deno run -A ./examples/ex5.ts",
    "ex6": "deno run -A ./examples/ex5.ts",
    "pub-dry": "deno publish --dry-run --allow-slow-types"
  },
  "fmt": {
    "exclude": [
      "./source/versions.ts"
    ]
  }
}